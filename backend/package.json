{
  "name": "log-lite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dashboard --port 8061 -- buildlite -j default~dev",
    "build": "webpack-dashboard --port 8061 -- buildlite -j default~build"
  },
  "buildlite": {
    "default": {
      "analyzer": 8005,
      "dashboard": 8061,
      "style": "style.css"
    },
    "dev": {
      "entry": {
        "index": "./setup/dev.index.jsx"
      },
      "react": "dev",
      "devServer": 8081,
      "devServerContentBase": "dev"
    },
    "build": {
      "entry": {
        "index": "./setup/dev.index.jsx"
      },
      "outputPath": "dev",
      "react": "build"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "express": "^4.16.2",
    "history": "^4.7.2",
    "json2csv": "^3.11.5",
    "mongodb": "^2.2.33"
  }
}
