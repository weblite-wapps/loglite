{
  "name": "log-lite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dashboard --port 8061 -- buildlite -j default~dev",
    "build": "webpack-dashboard --port 8061 -- buildlite -j default~build"
  },
  "buildlite": {
    "default": {
      "dashboard": 8061,
      "style": "style.css"
    },
    "dev": {
      "entry": {
        "index": "./setup/dev.index.jsx"
      },
      "react": "dev",
      "devServer": 8081,
      "devServerContentBase": "dev"
    },
    "build": {
      "watch": true,
      "entry": {
        "index": "./setup/dev.index.jsx"
      },
      "outputPath": "dev",
      "react": "build"
    }
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.26",
    "material-ui-icons": "^1.0.0-beta.17",
    "ramda": "^0.24.0",
    "react": "^16.1.1",
    "react-autocomplete": "^1.7.2",
    "react-csv": "^1.0.8",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "recharts": "^1.0.0-beta.7",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-observable": "^0.17.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.2",
    "superagent": "^3.8.1",
    "typeface-roboto": "^0.0.45"
  },
  "local-dependencies": [
    "weblite-web-snackbar"
  ]
}
